# plugin settings
set -U FZF_LEGACY_KEYBINDINGS 0
set -g theme_date_format "+%H:%M:%S"

# env
set -x ANDROID_HOME ~/Library/Android/sdk

# alias
alias gst 'git status'
alias ga 'git add'
alias gc 'git commit'
alias gb 'git branch'
alias gco 'git checkout'
alias gd 'git diff'
alias gf 'git fetch'
alias gdc 'git diff --cached'
alias gg 'git graph'
alias tmux 'tmux -u'
alias l 'ls -la'
alias mkdir 'mkdir -p'
